{
    "semi": false,
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 3,
    "useTabs": false,
    "jsxBracketSameLine": true,
    "arrowParens": "always",
    "quoteProps": "as-needed",
    "htmlWhitespaceSensitivity": "css",
    "vueIndentScriptAndStyle": false,
    "overrides": [
        {
            "files": "*.vue",
            "options": {
                "singleQuote": false
            }
        }
    ]
}
